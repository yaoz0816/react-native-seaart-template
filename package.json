{
  "name": "SeaArtRNTemplate",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "start": "npx react-native start --reset-cache",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "build:android:release:google:dev": "react-native build-android --tasks assembleRelease --extra-params -PreactNativeArchitectures=arm64-v8a",
    "build:android:release:google:prod": "react-native build-android --tasks assembleRelease --extra-params -PreactNativeArchitectures=arm64-v8a",
    "updateI18n": "rm -rf Tables && svn checkout svn://172.16.10.49/suclient/Sea_Art/Sea_Art_App/Tables/develop/excels Tables && node updateI18n.mjs && eslint src/locales --fix && prettier --write src/locales/**/*.json",
    "setup": "node scripts/setup.js",
    "template": "node scripts/create-template.js",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && rm -rf build && rm -rf Pods && rm Podfile.lock && pod install",
    "clean:ios:pnpm": "cd ios && rm -rf build && rm -rf Pods && rm Podfile.lock && pnpm exec pod install",
    "clean:ios:yarn": "cd ios && rm -rf build && rm -rf Pods && rm Podfile.lock && yarn pod install",
    "prepare-scaffold": "node scripts/prepare-scaffold.js",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "bundle:visualizer:android": "react-native-bundle-visualizer --platform android --dev false --reset-cache",
    "bundle:visualizer:ios": "react-native-bundle-visualizer --platform ios --dev false --reset-cache"
  },
